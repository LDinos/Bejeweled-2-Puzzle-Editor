/// @function script()
/// @description default
/// @param {type} param first parameter
function hint_convert_coord_to_string(j) {
	switch(j) {
		case 0: return "A";
		case 1: return "B";
		case 2: return "C";
		case 3: return "D";
		case 4: return "E";
		case 5: return "F";
		case 6: return "G";
		case 7: return "H";
	}
}